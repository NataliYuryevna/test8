          "_name": "footer1",
          "_customHTML": "<section class=\"footer1\" group=\"Footers\" plugins=\"amp-main, amp-form, amp-mustache\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" condition=\"showTitle || (showContacts &amp;&amp; (showAddress || showPhone || showEmail)) || showCategories || (showForm &amp;&amp; (showTitle || showFormText)) || (showLogoSocial &amp;&amp; (showLogo || showSocial || showLogoText))\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showTitle || (showContacts &amp;&amp; (showAddress || showPhone || showEmail)) || showCategories || (showForm &amp;&amp; (showTitle || showFormText)) || (showLogoSocial &amp;&amp; (showLogo || showSocial || showLogoText))\">\n    <header condition=\"showForm || showContacts || showCategories\">Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\" condition=\"showForm || showContacts || showCategories\">\n        <input type=\"checkbox\" title=\"Line Title\" name=\"showLineTitle\" condition=\" showTitle &amp;&amp; (showForm || showContacts || showCategories)\" checked=\"\">\n        <input type=\"checkbox\" title=\"Logo and Social\" name=\"showLogoSocial\" checked=\"\">\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\" checked=\"\">\n        <input type=\"checkbox\" title=\"Categories\" name=\"showCategories\" checked=\"\">\n        <input type=\"checkbox\" title=\"Form\" name=\"showForm\" checked=\"\">\n        <select title=\"Align\" name=\"align\" condition=\"showTitle || (showContacts &amp;&amp; (showAddress || showPhone || 