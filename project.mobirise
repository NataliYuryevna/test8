        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "amp-carousel .card": {
              "align-self": "flex-start"
            },
            ".slider-box, amp-carousel": {
              "height": "~\"@{slideHeight}px\" !important",
              "position": "relative"
            },
            ".card-box .card-subtitle when (@showCardsText) and (@showCardsTitle)": {
              "padding-top": "25px"
            },
            ".card-box when (@showCardsTitle), (@showCardsText)": {
              "padding-top": "45px"
            },
            ".card, .slider-box, amp-carousel, .dots-wrapper": {
              "width": "100%"
            },
            ".dots span, .amp-carousel-button": {
              "background-color": "@buttonsColor"
            },
            "& when not (@typeSlide = 'carousel')": {
              ".card": {
                "height": "~\"calc(@{slideHeight}px - 35px)\"",
                "margin-bottom": "35px"
              },
              ".dots-wrapper": {
                "position": "absolute",
                "bottom": "0"
              },
              ".dots span": {
                "display": "inline-block",
                "vertical-align": "middle",
                "border-radius": "10px",
                "width": "9px",
                "height": "9px",
                "margin": "0 3px",
                "opacity": "0.4",
                "outline": "none"
              },
              ".dots span.current": {
                "opacity": "1",
                "width": "18px"
              },
              ".amp-carousel-button": {
                "display": "none"
              }
            },
            "& when not (@showBullets), (@typeSlide = 'carousel')": {
              ".card when (@typeSlide = 'carousel'), (@slideWidth = 0), not (@showCenter)": {
                "height": "~\"calc(@{slideHeight}px - 85px)\"",
                "margin": "0 0 85px 0"
              },
              ".card when (@typeSlide = 'slides') and not (@slideWidth = 0) and (@showCenter)": {
                "height": "~\"@{slideHeight}px\""
              },
              ".amp-carousel-button": {
                "display": "block",
                "opacity": "0.8",
                "&:before, &:after": {
                  "color": "contrast(@buttonsColor, darken(@buttonsColor, 50%), #ffffff, 60%) !important"
                },
                "&:hover": {
                  "opacity": "1"
                }
              },
              ".amp-carousel-button when (@typeSlide = 'carousel'), (@slideWidth = 0), not (@showCenter)": {
                "top": "~\"calc(100% - 27px)\"",
                "&.amp-carousel-button-prev": {
                  "left": "15px"
                },
                "&.amp-carousel-button-next": {
                  "left": "70px"
                },
                "& when (@cardsAmount = 2) and (@typeSlide = 'carousel')": {
                  "@media (min-width: 1200px)": {
                    "&.amp-carousel-button-prev": {
                      "left": "30px"
                    },
                    "&.amp-carousel-button-next": {
                      "left": "(30px + 55px)"
                    }
                  }
                }
              },
              ".dots-wrapper": {
                "display": "none"
              },
              "& when not (@showBullets) and not (@typeSlide = 'carousel')": {
                ".card-box when (@showCenter)": {
                  "@media (min-width: 768px)": {
                    "padding-left": "60px",
                    "padding-right": "60px"
                  }
                },
                "@media (max-width: 767px)": {
                  ".dots-wrapper": {
                    "display": "flex"
                  },
                  ".amp-carousel-button": {
                    "display": "none"
                  },
                  ".card": {
                    "height": "~\"calc(@{slideHeight}px - 35px)\"",
                    "margin-bottom": "35px"
                  }
                }
              }
            },
            ".card-wrapper": {
              "height": "100%",
              "overflow": "hidden",
              ".card-box when (@showCardsTitle), (@showCardsText)": {
                "white-space": "normal"
              },
              "img": {
                "object-fit": "cover",
                "-webkit-transition": "transform 5s linear",
                "transition": "transform 5s linear"
              },
              "&:hover img": {
                "-webkit-transform": "scale(1.15)",
                "transform": "scale(1.15)"
              }
            },
            ".card-wrapper when not (@transparentBg)": {
              "background-color": "@cardBg"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-img amp-img": {
              "z-index": "2",
              "width": "~\"@{imageWidth}%\"",
              "min-width": "60px"
            },
            ".card .card-subtitle:before when (@lineTitle) and (@showCardsTitle)": {
              "content": "''",
              "display": "block",
              "width": "40px",
              "height": "3px",
              "margin": "inherit",
              "margin-bottom": "30px",
              "background-color": "@primaryColor",
              "& when (@cardAlign = '3')": {
                "@media (max-width: 767px)": {
                  "margin-right": "auto"
                }
              }
            },
            ".card-text": {
              "color": "#615779"
            }
          },
          "_name": "testimonials2",
          "_customHTML": "<section class=\"testimonials2 slider slider-with-bullets\" group=\"Testimonials\" plugins=\"amp-main, amp-carousel, amp-bind\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header mbr-if=\"showTitle\">Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Padding Slide\" name=\"slideWidth\" min=\"0\" max=\"5\" step=\"1\" value=\"3\" condition=\"typeSlide == 'slides'\">\n\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n\n    <header>Cards</header>\n        <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"typeSlide == 'carousel'\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showCardsTitle || showCardsText\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Name\" name=\"showCardsTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Title\" name=\"lineTitle\" condition=\"showCardsTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\">\n        <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#cccccc\" condition=\"showBorders\">\n        <input type=\"checkbox\" title=\"Transparent\" name=\"transparentBg\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#f4f5f7\" condition=\"!transparentBg\">\n\n    <header>Image</header>\n        <input type=\"checkbox\" title=\"Rounded\" name=\"roundedImage\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Width\" name=\"imageWidth\" min=\"10\" max=\"100\" step=\"1\" value=\"18\">\n\n    <header>Sliders</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" checked=\"\" condition=\"slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n            <input type=\"range\" inline=\"\" title=\"Delay\" name=\"delay\" min=\"1000\" max=\"5000\" step=\"1000\" value=\"3000\" condition=\"autoplay &amp;&amp; slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n        <input type=\"range\" inline=\"\" title=\"Slider Height\" name=\"slideHeight\" min=\"250\" max=\"500\" step=\"10\" value=\"440\">\n        <select title=\"Type Slide\" name=\"typeSlide\">\n            <option value=\"slides\">Slides</option>\n            <option value=\"carousel\" selected=\"\">Carousel</option>\n        </select>\n        <select title=\"Sliders Count\" name=\"slidersAmount\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\" selected=\"\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Buttons/Bullets\" name=\"showBullets\" condition=\"typeSlide == 'slides'\">\n            <input type=\"color\" title=\"Color\" name=\"buttonsColor\" value=\"#2f2447\">\n        <input type=\"checkbox\" title=\"Show Center Buttons\" name=\"showCenter\" condition=\"typeSlide == 'slides' &amp;&amp; !showBullets\" checked=\"\">\n        \n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/ca02.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#faf9f9\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f2eeef\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#f2eeef\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\