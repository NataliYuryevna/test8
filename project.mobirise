              "background-color": "@primaryColor"
            },
            "@media (max-width: 767px)": {
              ".card:not(.last-child) when (@showTeam)": {
                "padding-bottom": "20px"
              },
              ".mbr-jc-e": {
                "justify-content": "center"
              }
            },
            "@media (max-width: 991px)": {
              ".first-element": {
                "padding-bottom": "30px"
              }
            },
            ".card-img when (@showName), (@showSpecialty)": {
              "margin-right": "30px",
              "& amp-img, &": {
                "width": "~\"@{imageWidth}rem\"",
                "height": "auto",
                "@media (max-width: 767px)": {
                  "max-width": "80px"
                }
              }
            },
            ".marker-wrap:before when (@showFormList)": {
              "content": "\"\"",
              "display": "inline-block",
              "border": "3px solid @primaryColor",
              "width": "~\"calc(@{widthImg}rem - 6px)\"",
              "height": "~\"calc(@{widthImg}rem - 6px)\"",
              "box-sizing": "initial",
              "& when ((@widthImg * 16) > 40)": {
                "@media (max-width: 767px)": {
                  "width": "40px",
                  "height": "40px"
                }
              }
            },
            ".mbr-list .items-list, .contacts-box .mbr-flex, .card > .mbr-flex": {
              "flex-direction": "row"
            },
            ".mbr-text": {
              "width": "auto"
            },
            ".marker-wrap:before, .contacts-box .form-icon": {
              "margin-right": "15px"
            },
            ".mbr-list .items-list when (@showFormList)": {
              "align-items": "baseline",
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".form-icon .amp-iconfont when (@showIcons)": {
              "font-size": "~\"@{iconsSize}rem\"",
              "width": "~\"@{iconsSize}rem\"",
              "& when ((@iconsSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "font-size": "30px",
                  "width": "30px"
                }
              }
            },
            ".icons-list a when (@showSocial)": {
              "margin": "0"
            },
            ".social .amp-iconfont when (@showSocial)": {
              "font-size": "~\"calc(@{socialSize}rem * 0.42)\"",
              "line-height": "~\"calc(@{socialSize}rem - 4px)\"",
              "width": "~\"@{socialSize}rem\"",
              "height": "~\"@{socialSize}rem\"",
              "margin-bottom": "10px",
              "& when ((@socialSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "font-size": "~\"calc(30px * 0.42)\"",
                  "line-height": "26px",
                  "width": "30px",
                  "height": "30px"
                }
              }
            },
            ".form-block when (@transparentForm)": {
              "padding": "0"
            },
            ".form-wrap .mbr-overlay when not (@transparentForm)": {
              "background-color": "@formBgColor",
              "opacity": "@formBgOpacity"
            },
            ".mbr-form textarea": {
              "min-height": "130px"
            },
            ".mbr-row .card when not (@showPadding)": {
              "& when (@showBorders), not (@transparentBg)": {
                "padding": "0",
                "& when (@showBorders)": {
                  ".card-wrapper": {
                    "border": "none"
                  },
                  "@media (min-width: 768px)": {
                    "&:not(.last-child) .card-wrapper": {
                      "border-right": "1px solid @bordersColor"
                    },
                    "&.mbr-pb-4 .card-wrapper": {
                      "border-bottom": "1px solid @bordersColor"
                    },
                    "&.mbr-pb-4:nth-child(2) .card-wrapper": {
                      "border-right": "none"
                    }
                  },
                  "@media (max-width: 767px)": {
                    "&:not(.last-child) .card-wrapper": {
                      "border-right": "none",
                      "border-bottom": "1px solid @bordersColor"
                    }
                  }
                }
              }
            },
            ".items-list .mbr-text, .contacts-text": {
              "color": "#615779"
            }
          },
          "_name": "contacts2",
          "_customHTML": "<section class=\"contacts2\" group=\"Contacts\" plugins=\"amp-main, amp-form, amp-mustache\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n    <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n    <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n    <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n    <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showFormTitle || showFormSubtitle || showFormList || showFormText || showAddress || showPhone || showEmail || showSocial\">\n    <header>Title</header>\n    <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n    <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n    <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle\">\n        <option value=\"1\">Left</option>\n        <option value=\"2\" selected=\"\">Center</option>\n        <option value=\"3\">Right</option>\n    </select> \n    <header>Form</header>\n    <input type=\"range\" inline=\"\" title=\"Width Form\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"1\">   \n    <input type=\"color\" title=\"Form Text Color\" name=\"labelsColor\" value=\"#000000\">\n    <input type=\"checkbox\" title=\"Transparent\" name=\"transparentForm\" checked=\"\">    \n        <input type=\"color\" title=\"Form Bg Color\" name=\"formBgColor\" value=\"#efefef\" condition=\"!transparentForm\">\n        <input type=\"range\" title=\"Form Bg Opacity\" name=\"formBgOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"1\" condition=\"!transparentForm\">\n    <header>Team</header>\n        <input type=\"checkbox\" title=\"Show Team\" name=\"showTeam\">\n        <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"showTeam\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showTeam &amp;&amp; (showName || showSpecialty)\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Name\" name=\"showName\" checked=\"\" condition=\"showTeam\">\n        <input type=\"checkbox\" title=\"Show Specialty\" name=\"showSpecialty\" checked=\"\" condition=\"showTeam\">\n        <input type=\"checkbox\" title=\"Line Specialty\" name=\"lineSpecialty\" condition=\"showSpecialty &amp;&amp; showTeam\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Padding\" name=\"showPadding\" condition=\"(showBorders || !transparentBg) &amp;&amp; showTeam\">\n        <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\" condition=\"showTeam\">\n        <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#cccccc\" condition=\"showBorders &amp;&amp; showTeam\">\n        <input type=\"checkbox\" title=\"Transparent\" name=\"transparentBg\" checked=\"\" condition=\"showTeam\">\n            <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#f4f5f7\" condition=\"!transparentBg &amp;&amp; showTeam\">\n        <input type=\"checkbox\" title=\"Rounded\" name=\"roundedImage\" checked=\"\" condition=\"showTeam\">\n        <input type=\"range\" inline=\"\" title=\"Width\" name=\"imageWidth\" min=\"3\" max=\"8\" step=\"0.1\" value=\"4.9\" condition=\"(showName || showSpecialty) &amp;&amp; showTeam\">\n        <header>Contacts</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showFormTitle\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showFormSubtitle\">\n            <input type=\"checkbox\" title=\"List\" name=\"showFormList\" checked=\"\">\n                <input type=\"range\" inline=\"\" title=\"Count\" name=\"listCount\" min=\"1\" max=\"9\" step=\"1\" value=\"1\" condition=\"showFormList\">\n                <input type=\"range\" inline=\"\" title=\"Width Marker\" name=\"widthImg\" min=\"0.8\" max=\"3\" step=\"0.1\" value=\"0.8\" condition=\"showFormList\">\n                <input type=\"checkbox\" title=\"Title List\" name=\"showFormListTitle\" condition=\"showFormList\">\n                <input type=\"checkbox\" title=\"Text List\" name=\"showFormListText\" checked=\"\" condition=\"showFormList\">\n            <input type=\"checkbox\" title=\"Show Text\" name=\"showFormText\">\n            <input type=\"checkbox\" name=\"showAddress\" title=\"Address\" checked=\"\">\n            <input type=\"checkbox\" name=\"showPhone\" title=\"Phone\" checked=\"\">\n            <input type=\"checkbox\" name=\"showEmail\" title=\"E-mail\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" checked=\"\" condition=\"showAddress || showPhone || showEmail\">\n            <input type=\"range\" inline=\"\" title=\"Size\" name=\"iconsSize\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"1.4\" condition=\"showIcons\">\n            <input type=\"checkbox\" title=\"Show Social\" name=\"showSocial\" checked=\"\">\n                <input type=\"range\" inline=\"\" title=\"Count\" name=\"socialCount\" min=\"1\" max=\"10\" value=\"4\" step=\"1\" condition=\"showSocial\">\n                <input type=\"range\" inline=\"\" title=\"Size\" name=\"socialSize\" min=\"1.5\" max=\"5\" step=\"0.1\" value=\"2.7\" condition=\"showSocial\">\n            <select title=\"Align\" name=\"contactsAlign\" condition=\"showFormTitle || showFormSubtitle || showFormList || showFormText || showAddress || showPhone || showEmail || showSocial\">\n                <option value=\"1\" selected=\"\">Left</option>\n                <option value=\"2\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/ca02.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#fffcfd\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f2eeef\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#f2eeef\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left': titleAlign==1,\n                         'align-center': titleAlign==2,\n                         'align-right': titleAlign==3, 'mbr-px-2': fullWidth}\">\n            <h1 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">Request a call back</h1>\n            <h2 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">Lorem ipsum dolor sit amet, consectetur adipisicing elit.</h2>\n        </div>\n        <div class=\"mbr-row\" mbr-class=\"{'mbr-px-2': fullWidth}\">\n            <div class=\"form-container mbr-flex flex-wrap\" mbr-class=\"{'mbr-row-reverse':reverseContent &amp;&amp; (showFormTitle || showFormSubtitle || showFormList || showFormText || showAddress || showPhone || showEmail || showSocial)}\">\n                <div class=\"first-element mbr-flex mbr-col-sm-12 mbr-col-md-12\" mbr-class=\"{'mbr-col-lg-8':columnWidth==5,\n                             'mbr-col-lg-7':columnWidth==4,\n                             'mbr-col-lg-6':columnWidth==3,\n                             'mbr-col-lg-5':columnWidth==2,\n                             'mbr-col-lg-4':columnWidth==1}\" mbr-if=\"showFormTitle || showFormSubtitle || showFormList || showFormText || showAddress || showPhone || showEmail || showSocial\">\n                    <div class=\"first-block mbr-m-auto\" mbr-class=\"{'align-center': contactsAlign == 2,\n                                          'align-right': contactsAlign == 3}\">\n                        <h3 class=\"form-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showFormTitle\" data-app-selector=\".form-title\">Request a call back</h3>\n                        <h4 class=\"form-subtitle mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showFormSubtitle\" data-app-selector=\".form-subtitle\">of Company</h4>\n                        <div class=\"mbr-list\" mbr-list=\"\" mbr-list-grow=\"listCount\" mbr-if=\"showFormList\" mbr-class=\"{'mbr-pt-4': showFormTitle || showFromSubtitle}\">\n                            <div class=\"mbr-flex items-list\" mbr-class=\"{'mbr-jc-c': contactsAlign == 2, 'mbr-jc-e': contactsAlign == 3}\">\n                                <div class=\"marker-wrap\"></div>\n                                <div class=\"cotacts-row\" mbr-if=\"showFormListTitle || showFormListText\">\n                                    <h4 class=\"cotacts-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" mbr-if=\"showFormListTitle\" data-app-selector=\".items-list .cotacts-title\" mbr-class=\"{'mbr-pb-1':showFormListText}\">\n                                        First Title</h4>\n                                    <p class=\"mbr-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showFormListText\" data-app-selector=\".items-list .mbr-text\">\n                                       Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor.</p>\n                                 </div>\n                            </div> \n                        </div>\n                        <p class=\"mbr-text form-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showFormText\" data-app-selector=\".form-text\" mbr-class=\"{'mbr-pt-3':showFormTitle || showFormSubtitle || showFormList}\">\n                           Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p>\n                        <div class=\"contacts-box\" mbr-if=\"showAddress || showPhone || showEmail || showSocial\" mbr-class=\"{'mbr-pt-4':showFormTitle || showFormSubtitle || showFormList || showFormText}\">\n                        <div class=\"address mbr-flex\" mbr-if=\"showAddress\" mbr-class=\"{'mbr-jc-c': contactsAlign == 2, 'mbr-jc-e': contactsAlign == 3}\">\n                            <div class=\"form-icon\" mbr-if=\"showIcons\">\n                                <span mbr-icon=\"\" class=\"amp-iconfont fa-map-marker fa\"></span>\n                            </div>\n                            <p class=\"mbr-text contacts-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" data-app-selector=\".contacts-text\"><b>ADDRESS:</b><br><span mbr-link-type=\"address\">10 Street, Los Angeles, CA, 93050</span></p>\n                        </div>\n                        <div class=\"phone mbr-flex\" mbr-if=\"showPhone\" mbr-class=\"{'mbr-pt-3': showAddress, 'mbr-jc-c': contactsAlign == 2, 'mbr-jc-e': contactsAlign == 3}\">\n                            <div class=\"form-icon\" mbr-if=\"showIcons\">\n                                <span mbr-icon=\"\" class=\"amp-iconfont fa-volume-control-phone fa\"></span>\n                            </div>\n                            <p class=\"mbr-text contacts-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" data-app-selector=\".contacts-text\"><b>PHONE:</b><br><span mbr-link-type=\"phone\">1555-234-45-67<br>1234-567-89-12</span></p>\n                        </div>\n                        <div class=\"email mbr-flex\" mbr-if=\"showEmail\" mbr-class=\"{'mbr-pt-3': showPhone || showAddress, 'mbr-jc-c': contactsAlign == 2, 'mbr-jc-e': contactsAlign == 3}\">\n                            <div class=\"form-icon\" mbr-if=\"showIcons\">\n                                <span mbr-icon=\"\" class=\"amp-iconfont fa-envelope fa\"></span>\n                            </div>\n                            <p class=\"mbr-text contacts-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" data-app-selector=\".contacts-text\"><b>EMAIL:</b><br><span mbr-link-type=\"email\">demomail@test.org<br>demomail@test.org</span></p>\n                        </div>\n                        <div class=\"icons-list social\" mbr-if=\"showSocial\" mbr-list=\"\" mbr-list-grow=\"socialCount\" mbr-link-type=\"socials\" mbr-class=\"{'mbr-pt-4': showPhone || showAddress || showEmail}\">\n                            <a href=\"http://www.youtube.com/channel/UCt_tncVAetpK5JeM8L-8jyw\" target=\"_blank\">\n                                <span mbr-icon=\"\" class=\"amp-iconfont fa-youtube fa\"></span>\n                            </a>\n                            <a href=\"https://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\">\n                                <span mbr-icon=\"\" class=\"amp-iconfont fa-facebook-square fa\"></span>\n                            </a>\n                            <a href=\"https://plus.google.com/u/0/+Mobirise/posts\" target=\"_blank\">\n                                <span mbr-icon=\"\" class=\"amp-iconfont fa-google-plus-circle fa\"></span>\n                            </a>\n                            <a href=\"https://www.linkedin.com/in/mobirise\" target=\"_blank\">\n                                <span mbr-icon=\"\" class=\"amp-iconfont fa-linkedin-square fa\"></span>\n                            </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div mbr-form=\"\" class=\"form-wrap mbr-col-sm-12 mbr-col-md-12 mbr-flex\" mbr-class=\"{'mbr-col-lg-8':columnWidth==1,\n                                 'mbr-col-lg-7':columnWidth==2,\n                                 'mbr-col-lg-6':columnWidth==3,\n                                 'mbr-col-lg-5':columnWidth==4,\n                                 'mbr-col-lg-4':columnWidth==5,\n                                 'mbr-col-lg-12': !showFormTitle &amp;&amp; !showFormSubtitle &amp;&amp; !showFormList &amp;&amp; !showFormText &amp;&amp; !showAddress &amp;&amp; !showPhone &amp;&amp; !showEmail &amp;&amp; !showSocial}\">\n                    <div class=\"form-box mbr-m-auto\">\n                        <div class=\"mbr-row team-block mbr-pb-5\" mbr-if=\"showTeam\" mbr-class=\"{'mbr-px-2': (!transparentBg || showBorders) &amp;&amp; !showPadding, 'cardsAmount2': (transparentBg &amp;&amp; !showBorders) || ((!transparentBg || showBorders) &amp;&amp; showPadding)}\">\n                            <div class=\"card mbr-col-sm-12 mbr-col-md-6 mbr-col-xl-6\" mbr-class=\"{'mbr-pb-4':cardsAmount > 2, 'last-child': cardsAmount == 1}\">\n                                <div class=\"card-wrapper mbr-flex\" mbr-class=\"{'align-center mbr-jc-c': cardAlign==2,\n                                         'align-right mbr-jc-e': cardAlign==3, 'mbr-p-2': !transparentBg || showBorders}\">\n                                    <div class=\"card-img\">\n                                        <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage}\" src=\"@PROJECT_PATH@/assets/images/ca15.jpg\" layout=\"responsive\" width=\"100\" height=\"100\" alt=\"Mobirise\">\n                                            <div placeholder=\"\" class=\"placeholder\"></div>\n                                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/ca15.jpg\" alt=\"Mobirise\" title=\"\">\n                                        </amp-img>\n                                    </div>\n                                    <div class=\"card-box mbr-flex mbr-column mbr-jc-c\" mbr-if=\"showName || showSpecialty\">\n                                        <h3 class=\"card-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-6\" mbr-if=\"showName\" data-app-selector=\".card-title\" mbr-class=\"{'mbr-pb-1':showSpecialty}\">\n                                            Adelia Harrell</h3>\n                                        <h4 class=\"card-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSpecialty\" data-app-selector=\".card-subtitle\">Administrator &amp; Business Developer</h4>\n                                    </div>\n                                </div>\n                            </div>\n                            <!--  -->\n                            <div class=\"card mbr-col-sm-12 mbr-col-md-6 mbr-col-xl-6\" mbr-class=\"{'mbr-pb-4':cardsAmount > 2, 'last-child': cardsAmount == 2}\" mbr-if=\"cardsAmount > 1\">\n                                <div class=\"card-wrapper mbr-flex\" mbr-class=\"{'align-center mbr-jc-c': cardAlign==2,\n                                         'align-right mbr-jc-e': cardAlign==3, 'mbr-p-2': !transparentBg || showBorders}\">\n                                    <div class=\"card-img\">\n                                        <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage}\" src=\"@PROJECT_PATH@/assets/images/ca18.jpg\" layout=\"responsive\" width=\"100\" height=\"100\" alt=\"Mobirise\">\n                                            <div placeholder=\"\" class=\"placeholder\"></div>\n                                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/ca18.jpg\" alt=\"Mobirise\" title=\"\">\n                                        </amp-img>\n                                    </div>\n                                    <div class=\"card-box mbr-flex mbr-column mbr-jc-c\" mbr-if=\"showName || showSpecialty\">\n                                        <h3 class=\"card-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-6\" mbr-if=\"showName\" data-app-selector=\".card-title\" mbr-class=\"{'mbr-pb-1':showSpecialty}\">Robert Shaw</h3>\n                                        <h4 class=\"card-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSpecialty\" data-app-selector=\".card-subtitle\">Chief Marketing Officer</h4>\n                                    </div>\n                                </div>\n                            </div>\n                            <!--  -->\n                            <div class=\"card mbr-col-s